    <description>Initializes the view for a component and prepares it for presentation. This tag places the component bean, display view parameters and optionally the component xml in the specified scope.</description>
    <name>initComponent</name>
    <tag-class>com.vignette.ext.templating.taglib.InitComponentTagSupport</tag-class>
    <body-content>empty</body-content>
    <attribute>
      <description>The name with which the component bean will be stored in the specified scope.</description>
      <name>var</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <description>The name with which the display view parameter map of the component will be stored in the specified
        scope.</description>
      <name>params</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <description>The name with which the component's xml will be stored in the specified scope.</description>
      <name>xml</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
    <attribute>
      <description>The scope in which the information returned by the tag should be stored.</description>
      <name>scope</name>
      <required>false</required>
      <rtexprvalue>false</rtexprvalue>
    </attribute>
  </tag>
  <tag>
